@model IEnumerable<Key_Castle_Models.Seller_Profile>

<div class="container p-3">
    <div class="row p-4">
        <div class="col-6">
            <h2 class="text-primary">Seller's List</h2>
        </div>
    </div>
</div>

<br />
<br />

@if (Model.Count() > 0)
{
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Seller Id</th>
                <th>Shop Name</th>
                <th>Item Sold</th>
                <th>Photo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
            <tr>
                <td width="50%">@obj.Seller_id</td>
                <td width="30%">@obj.ShopName</td>
                <td width="30%">@obj.Item_sold</td>
                <td width="30%">@obj.Seller_photo</td>
            </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Nothing</p>
}


